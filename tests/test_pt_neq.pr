int main()
{
    /* creation and assignment */
    pt p1;
    pt p2;
    pt p3;
    poly crv;

    crv = [(5l, 12l) : 13l];
    p1 = [2l, 2l] & crv;
    p2 = [2l, 2l] & crv;
    p3 = [2l, 11l] & crv;
    print(p1 != p2);
    print(p2 != p1);
    print([2l, 2l] & crv != p1);
    print(p1 != p3);
    print(p3 != p1);
    print([2l, 2l] & crv != p3);
    return 0;
}
