int main()
{
    /* creation and assignment */
    pt p1;
    pt p2;
    pt p3;
    poly crv;

    crv = [(5l, 12l) : 13l];
    p1 = [2l, 2l] & crv;
    p2 = [7l, 0l] & crv;
    printpt(p2 + p1);
    p3 = p1 + p2;
    printpt(p3);
    printpt([2l, 2l] & [(5l, 12l) : 13l] + [7l, 0l] & [(5l, 12l) : 13l]);

    return 0;
}
